              dl.row.detail-list
                dt.col-sm-4 Tài khoản nhận
                dd.col-sm-8.text-left.bold 000528230005
                dt.col-sm-4 Chủ tài khoản
                dd.col-sm-8.text-left Do Minh Tri
                dt.col-sm-4 Ngân hàng
                dd.col-sm-8.text-left Techcombank chi nhánh Hoàn Kiếm
            div.text-center.mb-4
              a(href="#").mw-194.btn.orange.btn-lg-finhay TIẾP THEO
                img.arrow(src="images/icon-arrow.png", alt="icon")
            footer.mt-2
              a.more(href="#").d-block.text-center Huỷ
